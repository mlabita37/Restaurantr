<h2>Current Menu</h2>
<table>
  <thead>
    <tr>
        <th data-field="item_name">Name</th>
        <th data-field="price">Price</th>
        <th data-field="description">Description</th>
    </tr>
  </thead>
  <tbody>
<% @item.each do |item| %>
    <tr>
      <td><% item.item_name %></td>
      <td><%= item.description %></td>
      <td><%= item.price %></td>
    </tr>
  <% end %>
  </tbody>
</table>
