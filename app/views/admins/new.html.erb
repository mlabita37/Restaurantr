<%= stylesheet_link_tag   'admins', media: 'all', 'data-turbolinks-track' => true %>
<%= button_to 'HOME', '/', method: :get, class: "button-primary" %>
<h4>New Admin</h4>
<%= form_for @admin do |f| %>
<%= f.text_field :username %>
<%=f.text_field :password %>
  <%= f.submit :class => "button-primary" %>
<% end %>

<br>
<p>Already An Admin?</p>
<%= button_to 'Login', 'admin_path', method: :get, class: "button-primary" %>
