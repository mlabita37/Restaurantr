<%= stylesheet_link_tag   'admins', media: 'all', 'data-turbolinks-track' => true %>
<h4>New Admin</h4>
<%= form_for @admin do |f| %>
  <div class="row">
    <div class="four columns">
      <%= f.text_field :username %>
    </div>
  <div class="four columns">
    <%=f.text_field :password %>
  </div>
  <div class="four columns">
    <%= f.submit :class => "button submit" %>
  </div>
<% end %>
